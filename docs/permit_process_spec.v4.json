{
  "version": "v4",
  "arrivals": {
    "cases": {
      "small": 10,
      "medium": 800,
      "large": 6000
    },
    "dow_weights": {
      "mon": 0.25,
      "tue": 0.15,
      "wed": 0.15,
      "thu": 0.15,
      "fri": 0.2,
      "sat": 0.05,
      "sun": 0.05
    },
    "channel_by_dow": {
      "weekend_online_only": true,
      "online_share_weekday": 0.6
    }
  },
  "states": [
    {
      "id": "APP_SUBMIT",
      "working_hours": false,
      "duration": { "type": "lognormal", "mu": 30, "sigma": 15 },
      "resource_pool": [
        { "id": "system_auto", "weight": 1.0, "when": { "application_type": "online" } },
        { "id": "intake_clerk", "weight": 1.0, "when": { "application_type": "in_person" } }
      ]
    },
    {
      "id": "INITIAL_REVIEW",
      "working_hours": true,
      "duration": { "type": "lognormal", "mu": 240, "sigma": 120 },
      "resource_pool": [
        { "id": "intake_clerk_maria", "weight": 0.33 },
        { "id": "intake_clerk_pedro", "weight": 0.33 },
        { "id": "intake_clerk_ana", "weight": 0.34 }
      ],
      "branch": {
        "incomplete_rate": { "online": 0.15, "in_person": 0.25 },
        "ineligible_rate": 0.02
      }
    },
    {
      "id": "REQ_CHECK",
      "working_hours": true,
      "duration": { "type": "lognormal", "mu": 960, "sigma": 360 },
      "resource_pool": [
        { "id": "req_clerk_john", "weight": 0.4, "speed": 1.0, "reject_rate": 0.08 },
        { "id": "req_clerk_lisa", "weight": 0.35, "speed": 1.0, "reject_rate": 0.10 },
        { "id": "req_clerk_slow_tom", "weight": 0.25, "speed": 2.5, "reject_rate": 0.12 }
      ]
    },
    {
      "id": "HEALTH_INSPECTION",
      "working_hours": true,
      "duration": { "type": "lognormal", "mu": 2880, "sigma": 1440 },
      "resource_pool": [
        { "id": "inspector_garcia", "weight": 0.4, "reject_rate": 0.12, "manager_rate": 0.05 },
        { "id": "inspector_smith", "weight": 0.35, "reject_rate": 0.10, "manager_rate": 0.08 },
        { "id": "inspector_strict_jones", "weight": 0.25, "reject_rate": 0.25, "manager_rate": 0.15 }
      ]
    },
    { "id": "INFO_REQUEST", "working_hours": false, "duration": { "type": "fixed", "mins": 0 }, "resource_pool": [{ "id": "system_auto", "weight": 1 }] },
    { "id": "APPLICANT_RESPONSE", "working_hours": false, "duration": { "type": "lognormal", "mu": 7200, "sigma": 4320 }, "resource_pool": [{ "id": "applicant", "weight": 1 }] },
    { "id": "MANAGER_APPROVAL", "working_hours": true, "duration": { "type": "lognormal", "mu": 720, "sigma": 240 }, "resource_pool": [{ "id": "manager_rodriguez", "weight": 1 }] },
    { "id": "CARD_REQUEST", "working_hours": true, "duration": { "type": "lognormal", "mu": 60, "sigma": 30 }, "resource_pool": [{ "id": "system_auto", "weight": 1 }] },
    { "id": "CARD_PRODUCTION", "working_hours": true, "duration": { "type": "lognormal", "mu": 2880, "sigma": 1440 }, "resource_pool": [{ "id": "card_printer", "weight": 1 }] },
    { "id": "QUALITY_CHECK", "working_hours": true, "duration": { "type": "lognormal", "mu": 60, "sigma": 30 }, "resource_pool": [{ "id": "system_auto", "weight": 1 }] },
    { "id": "NOTIFY_APPLICANT", "working_hours": false, "duration": { "type": "lognormal", "mu": 20, "sigma": 10 }, "resource_pool": [{ "id": "system_auto", "weight": 1 }] },
    { "id": "PERMIT_DELIVERY", "working_hours": false, "duration": { "type": "choice", "download": 0, "postal_min": 1440, "postal_max": 7200 }, "resource_pool": [{ "id": "system_auto", "weight": 0.7 }, { "id": "postal_service", "weight": 0.3 }] },
    { "id": "REJECTED", "working_hours": true, "duration": { "type": "fixed", "mins": 0 }, "resource_pool": [{ "id": "decision_officer", "weight": 1 }] },
    { "id": "WITHDRAWN", "working_hours": false, "duration": { "type": "fixed", "mins": 0 }, "resource_pool": [{ "id": "applicant", "weight": 1 }] },
    { "id": "APPEAL_PROCESS", "working_hours": true, "duration": { "type": "lognormal", "mu": 28800, "sigma": 14400 }, "resource_pool": [{ "id": "manager_rodriguez", "weight": 1 }] }
  ],
  "transitions": [
    { "from": "APP_SUBMIT", "to": "INITIAL_REVIEW", "p": 1.0 },
    { "from": "INITIAL_REVIEW", "to": "INFO_REQUEST", "rule": "incomplete_by_channel" },
    { "from": "INITIAL_REVIEW", "to": "REQ_CHECK", "p_else": 0.98 },
    { "from": "INITIAL_REVIEW", "to": "REJECTED", "p": 0.02 },

    { "from": "INFO_REQUEST", "to": "APPLICANT_RESPONSE", "p": 1.0 },
    { "from": "APPLICANT_RESPONSE", "to": "REQ_CHECK", "p": 0.7 },
    { "from": "APPLICANT_RESPONSE", "to": "INITIAL_REVIEW", "p": 0.3 },

    { "from": "REQ_CHECK", "to": "HEALTH_INSPECTION", "p_else": 0.88 },
    { "from": "REQ_CHECK", "to": "INFO_REQUEST", "p": 0.02 },
    { "from": "REQ_CHECK", "to": "REJECTED", "rule": "reject_by_req_clerk" },

    { "from": "HEALTH_INSPECTION", "to": "REJECTED", "rule": "reject_by_inspector" },
    { "from": "HEALTH_INSPECTION", "to": "MANAGER_APPROVAL", "rule": "manager_by_inspector" },
    { "from": "HEALTH_INSPECTION", "to": "CARD_REQUEST", "p_else": 1.0 },

    { "from": "MANAGER_APPROVAL", "to": "CARD_REQUEST", "p": 1.0 },

    { "from": "CARD_REQUEST", "to": "CARD_PRODUCTION", "p": 1.0 },
    { "from": "CARD_PRODUCTION", "to": "QUALITY_CHECK", "p": 1.0 },
    { "from": "QUALITY_CHECK", "to": "NOTIFY_APPLICANT", "p": 1.0 },
    { "from": "NOTIFY_APPLICANT", "to": "PERMIT_DELIVERY", "p": 1.0 }
  ],
  "exceptions": {
    "withdrawn_rate": 0.05,
    "appeal_rate_of_rejected": 0.3
  },
  "capacities": {
    "CARD_PRODUCTION": { "daily_limit": 50, "max_delay_days": 5 },
    "MANAGER_APPROVAL": { "surge_delay_days_min": 3, "surge_delay_days_max": 10 }
  },
  "variants_target": {
    "happy_path": 0.6,
    "initial_incomplete_loop": 0.2,
    "req_issue_loop": 0.08,
    "inspection_manager": 0.07,
    "inspection_info_loop": 0.03,
    "req_reject": 0.015,
    "inspection_reject": 0.065
  }
}
